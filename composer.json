{
    "name": "novius/laravel-nova-form-builder",
    "description": "Form builder for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "form",
        "form-builder"
    ],
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^10.0 | ^11.0 | ^12.0",
        "laravel/nova": "^5.0",
        "laravel/nova-devtool": "^1.7",
        "novius/laravel-form": "^2.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0 | ^3.0",
        "laravel/pint": "^1.16",
        "orchestra/testbench": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Novius\\LaravelFormBuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Novius\\LaravelFormBuilder\\LaravelFormBuilderServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.novius.net:lara_libs/laravel-form.git"
        }
    ],
    "scripts": {
        "cs-fix": [
            "./vendor/bin/pint -v"
        ],
        "lint": [
            "@composer cs-fix -- --test"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse -c phpstan.dist.neon --memory-limit 1G"
        ],
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    }
}
